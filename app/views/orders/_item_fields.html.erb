<fieldset>  
  	<%= f.label :product, "Product" %>  
  	<%= f.collection_select :product_id, Product.all, :id, :name, class: 'form-control input-lg'%>  
  	<%= f.label :qty, "Quantity" %>  
  	<%= f.text_field :qty %>  
  	<%= f.hidden_field :_destroy %>  
  	<%= link_to "remove", '#', class: "remove_fields" %>  
 </fieldset>